{
  "name": "exabox",
  "version": "1.0.0",
  "description": "A peer-to-peer VoIP communications application with emphasis on file & screen sharing.",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none npm run react\" \"wait-on http://localhost:3000 && nodemon --exec electron .\"",
